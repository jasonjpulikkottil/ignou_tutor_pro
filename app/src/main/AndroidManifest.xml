<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.jdots.ignoupro">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="com.android.vending.BILLING" />

    <application
        android:name="androidx.multidex.MultiDexApplication"
        android:allowBackup="true"
        android:fullBackupContent="true"
        android:icon="@mipmap/icon_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true"
        tools:ignore="GoogleAppIndexingWarning"
        tools:replace="android:icon,android:theme"
        tools:targetApi="n">

		<meta-data android:name="com.facebook.sdk.ApplicationId" android:value="@string/facebook_app_id"/>

        <activity android:name=".login.LoginActivity">

            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.jdots.ignoupro.Main" />
        </activity>
        <activity android:name=".registration.RegistrationActivity">

        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value="com.jdots.ignoupro.login.LoginActivity" />
        </activity>
        <activity
            android:name=".main.MainActivity"
            android:label="@string/title_activity_main"
            android:theme="@style/AppTheme" />
        <activity android:name=".conversation.ConversationActivity">
            <intent-filter>
                <action android:name="OPEN_CONVERSATION" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".profile.ProfileActivity" />
        <activity android:name=".firstlogin.UserFirstLoginActivity" />

        <service
            android:name=".notifications.MyFirebaseMessagingService"
            android:exported="false">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
        <service
            android:name=".notifications.MyFirebaseInstanceIDService"
            android:exported="false">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT" />
            </intent-filter>
        </service>

        <activity
            android:name=".QnActivity"
            android:parentActivityName=".MaterialActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MaterialActivity" />
        </activity>
        <activity
            android:name=".MCAMaterialActivity"
            android:parentActivityName=".Main">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Main" />
        </activity>
        <activity
            android:name=".BAMaterialActivity"
            android:parentActivityName=".Main">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Main" />
        </activity>
        <activity
            android:name=".NoteListActivity"
            android:label="@string/app_name"
            android:parentActivityName=".MaterialActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MaterialActivity" />
        </activity>
        <activity
            android:name=".ModifyNoteActivity"
            android:label="@string/app_name"
            android:theme="@android:style/Theme.DeviceDefault.Light.Dialog" />
        <activity
            android:name=".AddNoteActivity"
            android:label="@string/app_name"
            android:theme="@android:style/Theme.DeviceDefault.Light.Dialog" />
        <activity
            android:name=".MaterialActivity"
            android:parentActivityName=".Main">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Main" />
        </activity>
        <activity
            android:name=".DocviewActivity"
            android:parentActivityName=".MaterialActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MaterialActivity" />
        </activity>
        <activity
            android:name=".WebActivity"
            android:parentActivityName=".Main">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Main" />
        </activity>
        <activity android:name=".Main" />

        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-2579135590725888~7362712136" />

        <activity
            android:name=".SettingsActivity"
            android:parentActivityName=".Main">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.jdots.ignoupro.Main" />
        </activity>
        <activity
            android:name=".PuzzleActivity"
            android:parentActivityName=".Main">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.jdots.ignoupro.Main" />
        </activity>
        <activity
            android:name=".Splash"
            android:configChanges="orientation|keyboardHidden"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>




 	<activity-alias
            android:name=".Pro"
            android:enabled="false"
       	android:icon="@mipmap/icon_launcher"
        android:label="@string/app_name_pro"
            android:targetActivity=".Main">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity-alias>

 	<activity-alias
            android:name=".Adfree"
            android:enabled="false"
       	android:icon="@mipmap/icon_launcher"
        android:label="@string/app_name_adfree"
            android:targetActivity=".Main">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity-alias>




    </application>

</manifest>